{
  "name": "@rohberg/volto-searchkit-block",
  "version": "0.1.0",
  "description": "volto-searchkit-block: Volto add-on",
  "main": "src/index.js",
  "author": "Katja SÃ¼ss, https://github.com/rohberg",
  "license": "MIT",
